{
  "name": "rec",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:production": "prisma generate && next build",
    "start": "next start",
    "start:production": "prisma db push && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "check-all": "pnpm type-check && pnpm lint && pnpm format:check",
    "fix-all": "pnpm lint:fix && pnpm format",
    "validate": "pnpm check-all && pnpm build",
    "db:seed": "tsx data/seeds/seed.ts",
    "db:reset": "prisma db push --force-reset && pnpm db:seed",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "prepare": "husky",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:migrate": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:reset": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "test:setup": "pnpm test:migrate && playwright install",
    "test:musicbrainz-mapping": "tsx scripts/test-musicbrainz-mapping.ts",
    "migrate:preview": "tsx scripts/migrate-discogs-to-musicbrainz.ts --dry-run",
    "migrate:execute": "tsx scripts/migrate-discogs-to-musicbrainz.ts --execute",
    "migrate:resume": "tsx scripts/migrate-discogs-to-musicbrainz.ts --resume",
    "queue:test-redis": "tsx src/lib/queue/test-redis.ts",
    "queue:test": "tsx src/lib/queue/test-queue.ts",
    "queue:test-service": "tsx src/lib/musicbrainz/test-queue-service.ts",
    "queue:debug": "tsx src/lib/queue/debug-job.ts",
    "queue:investigate": "tsx src/lib/queue/investigate-solutions.ts",
    "queue:activity": "tsx src/scripts/generate-queue-activity.ts",
    "queue:mock": "tsx src/scripts/test-queue-dashboard.ts",
    "queue:add": "tsx src/scripts/add-single-job.ts",
    "queue:slow": "tsx src/scripts/add-slow-job.ts",
    "queue:worker": "tsx src/scripts/start-worker.ts",
    "test:worker": "tsx src/scripts/test-worker.ts",
    "test:musicbrainz": "tsx src/scripts/test-musicbrainz.ts",
    "worker": "tsx src/workers/musicbrainz-worker.ts",
    "dashboard": "tsx src/scripts/bull-board-server.ts",
    "queue:dev": "concurrently \"pnpm dashboard\" \"pnpm worker\" --names \"dashboard,worker\" --prefix-colors \"cyan,yellow\""
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@auth/prisma-adapter": "^2.9.0",
    "@aws-sdk/client-s3": "^3.890.0",
    "@aws-sdk/s3-request-presigner": "^3.890.0",
    "@bull-board/api": "^6.12.7",
    "@bull-board/express": "^6.12.7",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
